generator client {
  provider = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id       String @id @default(uuid())
  email    String @unique(map: "User_email_key")
  name     String
  password String
}

model Agents {
  id        String   @id @default(uuid())
  image     String?
  name      String
  position  String
  email     String
  phone     String
  address   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
